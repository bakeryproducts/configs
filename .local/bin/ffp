#!/bin/bash
ssh $DB5USER@$DB5HOST -L 9000:localhost:9000 -N && \
ssh -p 9000 gsm@localhost -L 9088:localhost:9088 -N && \
ssh -p 9000 gsm@localhost -L 9006:localhost:9006 -N && \
ssh $DB7USER@$DB7HOST -L 8000:localhost:9000 -N && \
ssh -p 8000 gsm@localhost -L 8088:localhost:9088 -N && \
ssh -p 8000 gsm@localhost -L 8006:localhost:9006 -N 
